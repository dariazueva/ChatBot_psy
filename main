import telebot
from telebot import types

bot = telebot.TeleBot('–í–∞—à —Ç–æ–∫–µ–Ω')

@bot.message_handler(commands=['start'])
def start(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=3)
    item1 = types.KeyboardButton('üò°–ó–ª—é—Å—å')
    item2 = types.KeyboardButton('üò¢ –ì—Ä—É—â—É')
    item3 = types.KeyboardButton('üòü –¢—Ä–µ–≤–æ–∂—É—Å—å')
    item4 = types.KeyboardButton('üòï –°–∫—É—á–∞—é')
    item5 = types.KeyboardButton('üòê –°–ø–æ–∫–æ–µ–Ω')
    item6 = types.KeyboardButton('üòÅ –†–∞–¥—É—é—Å—å')
    markup.add(item1, item2, item3, item4, item5, item6)

    mess = f'–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ, <b>{message.from_user.first_name}!</b> –ö–∞–∫–æ–≤–æ –≤–∞—à–µ —ç–º–æ—Ü–∏–æ–Ω–∞–ª—å–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ —Å–µ–π—á–∞—Å?'
    bot.send_message(message.chat.id, mess, parse_mode='html', reply_markup=markup)

@bot.message_handler(content_types=['text'])
def bot_message(message):
    if message.chat.type == 'private':
        if message.text == 'üò°–ó–ª—é—Å—å':
            markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=2)
