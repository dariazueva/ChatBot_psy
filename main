            item3=types.KeyboardButton('‚òÅÔ∏è –ú–µ–¥–∏—Ç–∞—Ü–∏—è')
            item4=types.KeyboardButton('–ù–∞–∑–∞–¥')
            markup.add(item1, item2, item3, item4)

